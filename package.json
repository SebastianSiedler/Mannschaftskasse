{
  "name": "mannschaftskasse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:1-generate": "prisma generate",
    "build:2-migrate": "prisma migrate deploy",
    "build:3-build": "next build",
    "build": "run-s build:*",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "yarn prisma migrate dev",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dev": "next dev",
    "dx": "cross-env DATABASE_URL=postgresql://postgres:@localhost:5632/trpcdb run-s db-up db-migrate-dev db-seed dev",
    "start": "next start",
    "lint": "eslint src",
    "lint-fix": "yarn lint --fix",
    "test-dev": "start-server-and-test dev 3000 test",
    "test-start": "start-server-and-test start 3000 test",
    "test": "run-s test:*",
    "test:unit": "jest",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "yarn ts-node prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
